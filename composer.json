{
    "name": "ajency/commonfloor",
    "description": "Commonfloor.com apartment selector.",
    "keywords": ["ajency", "commonfloor", "apartment selector"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "5.0.*",
        "barryvdh/laravel-ide-helper": "~2.0",
        "illuminate/html": "5.0.*@dev",
        "jildertmiedema/laravel-plupload": "dev-master",
        "masterexploder/phpthumb" : "*",
        "zizaco/entrust": "dev-laravel-5",
        "chrisbjr/api-guard": "~2.0",
        "maatwebsite/excel": "~2.0.0"
        

    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "league/factory-muffin": "*",
        "codeception/codeception": "*",
        "janhenkgerritsen/codeception-laravel5": "~1.0",
        "doctrine/dbal": "*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "CommonFloor\\": "app/"
        },
        "files" : [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
